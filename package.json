{
  "name": "host-helper-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "start": "vite --mode development",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint src --report-unused-disable-directives",
    "lint:errors": "eslint src --report-unused-disable-directives --quiet",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "lint:fix": "eslint src --fix --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "npm run build && npm run preview:prod",
    "fix:all": "eslint src --fix && prettier --write src",
    "fix:imports": "eslint src --fix --rule \"@typescript-eslint/no-unused-vars: error\"",
    "fix:any": "eslint src --fix --rule \"@typescript-eslint/no-explicit-any: error\"",
    "fix:hooks": "eslint src --fix --rule \"react-hooks/exhaustive-deps: error\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@types/uuid": "^9.0.8",
    "date-fns": "^3.6.0",
    "i18next": "^23.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.0",
    "react-router-dom": "^6.22.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@eslint/js": "^8.57.0",
    "@heroicons/react": "^2.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "esbuild": "^0.25.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^14.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0",
    "vite": "^6.2.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.12.0"
  }
}
