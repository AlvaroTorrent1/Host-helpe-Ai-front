{
  "name": "host-helper-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:prod": "vite --mode production",
    "start": "vite --mode development",
    "build": "tsc && vite build --mode production",
    "build:dev": "tsc && vite build --mode development",
    "lint": "eslint src --report-unused-disable-directives",
    "lint:errors": "eslint src --report-unused-disable-directives --quiet",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "lint:fix": "eslint src --fix --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "deploy": "npm run build && npm run preview:prod",
    "verify:production": "node scripts/verify-stripe-production.js",
    "fix:all": "eslint src --fix && prettier --write src",
    "fix:imports": "eslint src --fix --rule \"@typescript-eslint/no-unused-vars: error\"",
    "fix:any": "eslint src --fix --rule \"@typescript-eslint/no-explicit-any: error\"",
    "fix:hooks": "eslint src --fix --rule \"react-hooks/exhaustive-deps: error\"",
    "pre-deploy": "npm run verify:production && npm run lint:errors && npm run build",
    "deploy:production": "npm run pre-deploy && echo 'âœ… Build exitoso. Proceder con despliegue manual siguiendo PRODUCTION_SETUP.md'",
    "check:stripe": "echo 'ðŸ”‘ Verificar que las claves de Stripe sean LIVE (pk_live_*, sk_live_*)",
    "stripe:test": "node scripts/stripe-mode-switcher.cjs 1",
    "stripe:demo": "node scripts/stripe-mode-switcher.cjs 2",
    "stripe:production": "node scripts/stripe-mode-switcher.cjs 3",
    "stripe:status": "node scripts/stripe-mode-switcher.cjs",
    "stripe:help": "echo 'Comandos: stripe:test, stripe:demo, stripe:production, stripe:status'",
    "test:production": "node scripts/test-production-flow.cjs",
    "verify:config": "node scripts/verify-config.cjs",
    "diagnose:oauth": "node scripts/diagnose-oauth-redirect.cjs",
    "analyze": "npm run build && npx vite-bundle-visualizer",
    "perf:check": "echo 'ðŸš€ Ejecuta: npm run build && npx serve dist -p 5000 && ejecuta Lighthouse en Chrome DevTools'"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.0",
    "@supabase/supabase-js": "^2.49.6",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "form-data": "^4.0.4",
    "framer-motion": "12.23.12",
    "i18next": "^23.16.8",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^6.22.3",
    "recharts": "^3.1.2",
    "stripe": "^18.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@eslint/js": "^8.57.0",
    "@heroicons/react": "^2.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/chai": "^5.2.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/sinon": "^17.0.4",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "esbuild": "^0.25.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^14.0.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.2.0",
    "vite": "^6.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "web-vitals": "^5.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.12.0"
  }
}
