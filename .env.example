# Environment
NODE_ENV=development

# Supabase Configuration
REACT_APP_SUPABASE_URL=https://your-supabase-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key
REACT_APP_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
REACT_APP_SUPABASE_STORAGE_URL=https://your-supabase-project.supabase.co/storage/v1/object/public/

# API Configuration
REACT_APP_API_URL=https://api.example.com

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_MODE=true

# Application Settings
REACT_APP_DEFAULT_LOCALE=en
REACT_APP_DEFAULT_CURRENCY=EUR

# Credenciales de Supabase (configuraciÃ³n de ejemplo)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Site URL - CRITICAL for email authentication and redirect URLs
# For development: Use localhost with correct port (same as your Vite server)
# For production: Use your actual domain (e.g., https://hosthelperai.com)
# Important: This must match the "Site URL" configured in your Supabase dashboard under Authentication > URL Configuration
VITE_SITE_URL=http://localhost:4000

# Supabase Authentication Settings
# Important: Make sure to add these URLs to the "Redirect URLs" in your Supabase dashboard
# under Authentication > URL Configuration as allowed redirect URLs:
# - http://localhost:4000/auth/callback (for development)
# - https://hosthelperai.com/auth/callback (for production)

# Feature Flags
VITE_ENABLE_CHATBOT=true
VITE_ENABLE_ANALYTICS=false

# App Configuration
VITE_APP_NAME="Host Helper AI"
VITE_APP_VERSION="1.0.0"
VITE_DEFAULT_LANGUAGE="en"

# API Keys (Add any additional external service API keys here)
# VITE_MAPS_API_KEY=your_maps_api_key
# VITE_ANALYTICS_ID=your_analytics_id

# Development Settings
VITE_DEV_API_DELAY=0 # Simulated API delay in milliseconds (for development)
VITE_DEBUG_MODE=false # Enable additional console logging for debugging

